#!/usr/bin/env runghc

--
-- More combining unicode characters 
-- http://www.fileformat.info/info/unicode/char/search.htm?q=combining&preview=entity
--

epsilon = "\x3B5"
latinSmallOpenE = "\x25B"
latinCapitalOpenE = "\x190"
cursorLeft = "\x202E"
combiningLongSolidus = "\x338"
combiningShortSolidus = "\x337"
slash = "\x02F"

main = putStrLn $ latinSmallOpenE ++ combiningLongSolidus
